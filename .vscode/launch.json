{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "runtimeArgs": [
        "--remote-debugging-port=9222",
        "--load-extension=${workspaceFolder}", // this is the path to the extension folder
        "chrome://extensions",
        // "data:text/html;charset=UTF-8,<html><head><title>test</title><body><ul><li><a href=\"https://learn.microsoft.com/en-us/azure/?product=popular\">Microsoft Learn</a></li><li><a href=\"https://docs.aws.amazon.com/en_us/#welcome\">AWS</a></li><li><a href=\"https://cloud.google.com/why-google-cloud/?hl=en&gclsrc=aw.ds\">Google Cloud</a></li><li><a href=\"https://docs.github.com/en/get-started\">Github</a></li></ul></body></html>",
        "data:text/html;charset=UTF-8;base64,"
      ],
      "env": {
        "CHROME_DEVELOPER_TOOLS": true
      },
      "preLaunchTask": "build"
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch Edge",
      "runtimeArgs": [
        "--remote-debugging-port=9222",
        "--load-extension=${workspaceFolder}", // this is the path to the extension folder
        "chrome://extensions",
        // "data:text/html;charset=UTF-8,<html><head><title>test</title><body><ul><li><a href=\"https://learn.microsoft.com/en-us/azure/?product=popular\">Microsoft Learn</a></li><li><a href=\"https://docs.aws.amazon.com/en_us/#welcome\">AWS</a></li><li><a href=\"https://cloud.google.com/why-google-cloud/?hl=en&gclsrc=aw.ds\">Google Cloud</a></li><li><a href=\"https://docs.github.com/en/get-started\">Github</a></li></ul></body></html>",
        "data:text/html;charset=UTF-8;base64,"
      ],
      "env": {
        "CHROME_DEVELOPER_TOOLS": true
      },
      "preLaunchTask": "build"
    }
  ]
}